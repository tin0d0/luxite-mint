    "wrap": "npx ts-node scripts/wrap.ts",
    "unwrap": "npx ts-node scripts/unwrap.ts",
    "status": "npx ts-node scripts/status.ts",
    "grind": "npx ts-node scripts/grind-vanity.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
