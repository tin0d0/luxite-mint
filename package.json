{
  "name": "luxite-mint",
  "version": "0.1.0",
  "description": "LUXITE Mint - LSQ to LUXITE 1:1 token wrapper",
  "scripts": {
    "build": "anchor build",
    "test": "anchor test",
    "test:local": "anchor test --skip-local-validator",
