    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@metaplex-foundation/umi-signer-wallet-adapters": "^1.4.1",
    "@solana/spl-token": "^0.4.6",
    "@solana/web3.js": "^1.91.8"
  },
  "devDependencies": {
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.12.7",
    "chai": "^4.5.0",
    "mocha": "^10.8.2",
    "ts-mocha": "^10.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
